{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "globals": {
        "assert": "readonly"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "indent": ["error", 4],
        "no-console": "off"
    }
}
